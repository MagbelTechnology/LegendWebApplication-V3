     am_assetTransfer."OLD_DEPT_CODE" AS am_assetTransfer_OLD_DEPT_CODE,
     am_ad_branch_A."BRANCH_NAME" AS am_ad_branch_A_BRANCH_NAME,
     am_assetTransfer."approval_status" AS am_assetTransfer_approval_status,
     am_assetTransfer."OLD_CATEGORY_CODE" AS am_assetTransfer_OLD_CATEGORY_CODE,
     am_assetTransfer."description" AS am_assetTransfer_description,
