	<queryString>
		<![CDATA[SELECT
     HD_COMPLAINT."ID" AS HD_COMPLAINT_ID,
     HD_COMPLAINT."COMPLAINT_ID" AS HD_COMPLAINT_COMPLAINT_ID,
     HD_COMPLAINT."COMPLAIN_CATEGORY" AS HD_COMPLAINT_COMPLAIN_CATEGORY,
     HD_COMPLAINT."COMPLAIN_SUB_CATEGORY" AS HD_COMPLAINT_complain_sub_cat,
     HD_COMPLAINT."ASSET_ID" AS HD_COMPLAINT_ASSET_ID,
     HD_COMPLAINT."PRIORITY" AS HD_COMPLAINT_PRIORITY,
     HD_COMPLAINT."CREATE_DATE" AS HD_COMPLAINT_CREATE_DATE,
     HD_COMPLAINT."STATUS" AS HD_COMPLAINT_STATUS,
     HD_COMPLAINT."COMPLAINT_TYPE" AS HD_COMPLAINT_COMPLAINT_TYPE,
     HD_COMPLAINT."INCIDENT_MODE" AS HD_COMPLAINT_INCIDENT_MODE,
     HD_COMPLAINT."USER_ID" AS HD_COMPLAINT_USER_ID,
     HD_COMPLAINT."RESPONSE_MODE" AS HD_COMPLAINT_RESPONSE_MODE,
     HD_COMPLAINT."REQUESTER_NAME" AS HD_COMPLAINT_REQUESTER_NAME,
     HD_COMPLAINT."REQUESTER_CONTACT_NO" AS HD_COMPLAINT_REQUESTER_CONTACT,
     HD_COMPLAINT."WORK_STATION_IP" AS HD_COMPLAINT_WORK_STATION_IP,
     HD_COMPLAINT."NOTIFY_EMAIL" AS HD_COMPLAINT_NOTIFY_EMAIL,
     HD_COMPLAINT."REQUEST_BRANCH" AS HD_COMPLAINT_REQUEST_BRANCH,
     HD_COMPLAINT."REQUEST_DEPARTMENT" AS HD_COMPLAINT_REQUEST_DEPARTMEN,
     HD_COMPLAINT."COMPANY_CODE" AS HD_COMPLAINT_COMPANY_CODE,
     HD_COMPLAINT."IMAGE" AS HD_COMPLAINT_IMAGE,
     HD_COMPLAINT."REQUEST_SECTION" AS HD_COMPLAINT_REQUEST_SECTION,
     HD_COMPLAINT."REQUEST_SUBJECT" AS HD_COMPLAINT_REQUEST_SUBJECT,
     HD_COMPLAINT."REQUEST_DESCRIPTION" AS HD_COMPLAINT_request_Descript,
     HD_COMPLAINT."CREATE_TIME" AS create_time,
     HD_COMPLAINT."REQUESTER_ID" AS requester_id,
     HD_COMPLAINT."TECHNICIAN" AS technician,
     HD_COMPLAINT."NATURE" AS nature,
     HD_COMPLAINT."DUE_DATE" AS Due_Date,
     HD_COMPLAINT."RESP_DATE" AS Resp_Date,
     HD_COMPLAINT."SLA_ID" AS sla_id,
     HD_COMPLAINT."MAIL_STATUS" AS STATUS_DESCRIPTION,
     HD_COMPLAINT."CREATE_DATETIME" AS HD_COMPLAINT_CREATE_DATETIME,
     HD_COMPLAINT."CLOSE_DATE" AS HD_COMPLAINT_CLOSE_DATE,
     HD_STATUS."STATUS_DESCRIPTION" AS HD_STATUS_STATUS_DESCRIPTION,
     HD_PRIORITY."PRIORITY_DESCRIPTION" AS HD_PRIORITY_PRIORITY_DESCRIPTI,
     AM_AD_DEPARTMENT."DEPT_NAME" AS Dept_name,
     HD_COMPLAIN_SUBCATEGORY."SUB_CATEGORY_NAME" AS sub_category_name,
     AM_GB_USER."FULL_NAME" AS Full_Name,
     AM_GB_USER_A."FULL_NAME" AS AM_GB_USER_A_FULL_NAME,
     HD_COMPLAINT_TYPE."DESCRIPTION" AS description,
     AM_GB_COMPANY."COMPANY_NAME" AS company_name
FROM
     "HELPDESK"."HD_COMPLAINT" HD_COMPLAINT INNER JOIN "HELPDESK"."AM_GB_USER" AM_GB_USER ON HD_COMPLAINT."USER_ID" = AM_GB_USER."USER_ID"
     INNER JOIN "HELPDESK"."AM_GB_USER" AM_GB_USER_A ON HD_COMPLAINT."TECHNICIAN" = AM_GB_USER_A."USER_ID"
     INNER JOIN "HELPDESK"."HD_STATUS" HD_STATUS ON HD_COMPLAINT."STATUS" = HD_STATUS."STATUS_CODE"
     INNER JOIN "HELPDESK"."HD_PRIORITY" HD_PRIORITY ON HD_COMPLAINT."PRIORITY" = HD_PRIORITY."PRIORITY_CODE"
     INNER JOIN "HELPDESK"."AM_AD_DEPARTMENT" AM_AD_DEPARTMENT ON HD_COMPLAINT."COMPLAIN_CATEGORY" = AM_AD_DEPARTMENT."DEPT_CODE"
     INNER JOIN "HELPDESK"."HD_COMPLAIN_SUBCATEGORY" HD_COMPLAIN_SUBCATEGORY ON HD_COMPLAINT."COMPLAIN_SUB_CATEGORY" = HD_COMPLAIN_SUBCATEGORY."SUB_CATEGORY_CODE"
     INNER JOIN "HELPDESK"."HD_COMPLAINT_TYPE" HD_COMPLAINT_TYPE ON HD_COMPLAINT."COMPLAINT_TYPE" = HD_COMPLAINT_TYPE."COMPLAINT_TYPE_CODE",
     "HELPDESK"."AM_GB_COMPANY" AM_GB_COMPANY
WHERE
     HD_COMPLAINT."STATUS" = $P!{Status}
     and HD_COMPLAINT."CREATE_DATE" BETWEEN $P!{startDate}and$P!{endDate}]]>
