     am_asset_transfer."OLD_BRANCH_CODE" AS am_asset_transfer_OLD_BRANCH_CODE,
     am_asset_transfer."OLD_SECTION_CODE" AS am_asset_transfer_OLD_SECTION_CODE,
     am_asset_transfer."OLD_DEPT_CODE" AS am_asset_transfer_OLD_DEPT_CODE,
     am_ad_branch_A."BRANCH_NAME" AS am_ad_branch_A_BRANCH_NAME,
     am_asset_transfer."approval_status" AS am_asset_transfer_approval_status,
     am_asset_transfer."OLD_CATEGORY_CODE" AS am_asset_transfer_OLD_CATEGORY_CODE,
     am_asset_transfer."description" AS am_asset_transfer_description,
     am_asset_transfer."cost_price" AS am_asset_transfer_cost_price,
     am_asset_transfer."Accum_Dep" AS am_asset_transfer_Accum_Dep,
     am_asset_transfer."Monthly_Dep" AS am_asset_transfer_Monthly_Dep,
     am_asset_transfer."NEW_BRANCH_CODE" AS am_asset_transfer_NEW_BRANCH_CODE,
     am_asset_transfer."NEW_SECTION_CODE" AS am_asset_transfer_NEW_SECTION_CODE,
     am_asset_transfer."NEW_DEPT_CODE" AS am_asset_transfer_NEW_DEPT_CODE,
     am_asset_transfer."NBV" AS am_asset_transfer_NBV,
     am_asset_transfer."New_Asset_id" AS am_asset_transfer_New_Asset_id,
     am_ad_branch_A."BRANCH_ID" AS am_ad_branch_A_BRANCH_ID
FROM
     "dbo"."am_asset_transfer" am_asset_transfer INNER JOIN "dbo"."am_ad_branch" am_ad_branch_A ON am_asset_transfer."OLD_BRANCH_CODE" = am_ad_branch_A."BRANCH_CODE"
