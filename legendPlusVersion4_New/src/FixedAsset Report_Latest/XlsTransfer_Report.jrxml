     "dbo"."am_gb_company" am_gb_company
WHERE
     am_asset_transfer."approval_status" = 'ACTIVE' $P!{filterClause}
ORDER BY
     am_asset_transfer."OLD_BRANCH_CODE" ASC,
     am_asset_transfer."OLD_CATEGORY_CODE" ASC]]>
