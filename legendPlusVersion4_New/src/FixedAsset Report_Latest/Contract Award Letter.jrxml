     "dbo"."PR_REQUEST_FOR_QUOTATION" PR_REQUEST_FOR_QUOTATION INNER JOIN "dbo"."am_ad_vendor" am_ad_vendor ON PR_REQUEST_FOR_QUOTATION."Vendor_Code" = am_ad_vendor."Vendor_Code"
