	<queryString>
		<![CDATA[SELECT distinct
     am_gb_company."company_name" AS am_gb_company_company_name,
     am_ad_branch."BRANCH_NAME" AS am_ad_branch_BRANCH_NAME,
     am_ad_category."category_name" AS am_ad_category_category_name,
     am_ad_category."Dep_rate" AS am_ad_category_Dep_rate,
     am_ad_category."Accum_Dep_ledger" AS am_ad_category_Accum_Dep_ledger,
     am_ad_category."Dep_ledger" AS am_ad_category_Dep_ledger,
     am_ad_category."Asset_Ledger" AS am_ad_category_Asset_Ledger,
     am_ad_category."gl_account" AS am_ad_category_gl_account,
     am_asset_improvement."asset_id" AS am_asset_improvement_asset_id,
     am_asset_improvement."description" AS am_asset_improvement_description,
     am_asset_improvement."old_accum_dep" AS am_asset_improvement_old_accum_dep,
     am_asset_improvement."old_cost_price" AS am_asset_improvement_old_cost_price,
     am_asset_improvement."new_cost_price" AS am_asset_improvement_new_cost_price,
     am_asset_improvement."old_nbv" AS am_asset_improvement_old_nbv,
     am_asset_improvement."branch_code" AS am_asset_improvement_branch_code,
     am_asset_improvement."category_code" AS am_asset_improvement_category_code,
     am_asset_improvement."cost_increase" AS am_asset_improvement_cost_increase,
     am_asset_improvement."revalue_reason" AS am_asset_improvement_revalue_reason,
     am_asset_improvement."revalue_Date" AS am_asset_improvement_revalue_Date,
     am_asset_improvement."cost_price" AS am_asset_improvement_cost_price,
     am_asset_improvement."Revalue_ID" AS am_asset_improvement_Revalue_ID,
     am_asset_improvement."approval_status" AS am_asset_improvement_approval_status,
     am_asset_improvement."effDate" AS am_asset_improvement_effDate,
     AM_ASSET."Finacle_Posted_Date" AS AM_ASSET_Finacle_Posted_Date,
     am_raisentry_transaction_view."iso" AS am_raisentry_transaction_view_iso,
     am_raisentry_transaction_view."page1" AS am_raisentry_transaction_view_page1
    -- AM_ASSET."Asset_Status" AS AM_ASSET_Asset_Status
FROM
     "dbo"."am_asset_improvement" am_asset_improvement INNER JOIN "dbo"."am_ad_branch" am_ad_branch ON am_asset_improvement."branch_code" = am_ad_branch."BRANCH_CODE"
     INNER JOIN "dbo"."am_ad_category" am_ad_category ON am_asset_improvement."category_code" = am_ad_category."category_code"
     INNER JOIN "dbo"."AM_ASSET" AM_ASSET ON am_asset_improvement."asset_id" = AM_ASSET."Asset_id"
     INNER JOIN "dbo"."am_raisentry_transaction_view" am_raisentry_transaction_view ON am_asset_improvement."asset_id" = am_raisentry_transaction_view."asset_id",
     "dbo"."am_gb_company" am_gb_company
WHERE
     am_asset_improvement."approval_status" = 'ACTIVE' $P!{filterClause}
	 and am_raisentry_transaction_view."iso" = '000'
	 and am_asset_improvement."category_code" != '013'
	 and am_raisentry_transaction_view."page1" = 'ASSET IMPROVEMENT RAISE ENTRY'
	 and AM_ASSET."Asset_Status"  = 'ACTIVE'
	 and am_raisentry_transaction_view."transactionId" =26]]>
