b.Monthly_Dep AS Prev_Monthly_Dep,b.Accum_Dep AS Prev_Accum_Dep,b.Cost_Price AS Prev_Cost_Price,
b.NBV AS Prev_NBV,a.Monthly_Dep-b.Monthly_Dep AS Monthly_Difference,a.Accum_Dep-b.Accum_Dep AS Accum_Difference,
a.NBV-b.NBV AS NBV_Difference, b.IMPROV_Monthly_Dep AS Prev_IMPROV_Monthly_Dep,
b.IMPROV_Accum_Dep AS Prev_IMPROV_Accum_Dep,b.IMPROV_COST AS Prev_IMPROV_COST,
b.IMPROV_NBV AS Prev_IMPROV_NBV,a.IMPROV_Monthly_Dep-b.IMPROV_Monthly_Dep AS Prev_Monthly_Difference
from Depreciation_Comparism_Current a, Depreciation_Comparism_Previous b, am_ad_category c
 where a.Category_Code = c.Category_Code and a.Asset_Id = b.Asset_Id
 UNION
select Asset_Id, Branch_Code,a.Category_Code, c.category_name,Monthly_Dep,Accum_Dep,Cost_Price,NBV,IMPROV_Monthly_Dep,
IMPROV_Accum_Dep,IMPROV_COST,IMPROV_NBV,0.00 AS bMonthly_Dep,0.00 AS bAccum_Dep,
0.00 AS bCost_Price,0.00 AS bNBV,Monthly_Dep AS Monthly_Difference,Accum_Dep AS Accum_Difference,
NBV AS NBV_Difference, 0.00 AS Prev_IMPROV_Monthly_Dep,
0.00 AS Prev_IMPROV_Accum_Dep,0.00 AS Prev_IMPROV_COST,
0.00 AS Prev_IMPROV_NBV,IMPROV_Monthly_Dep AS Prev_Monthly_Difference from Depreciation_Comparism_Current a, am_ad_category c
where a.Asset_Id not in (select Asset_Id from Depreciation_Comparism_Previous)
and a.Category_Code = c.category_code]]>
