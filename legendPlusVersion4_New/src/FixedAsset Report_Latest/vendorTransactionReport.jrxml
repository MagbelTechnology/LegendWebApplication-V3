     VENDOR_TRANSACTIONS."CRACCOUNT_NO" AS VENDOR_TRANSACTIONS_CRACCOUNT_NO,
     VENDOR_TRANSACTIONS."PROJECT_CODE" AS VENDOR_TRANSACTIONS_PROJECT_CODE,
     VENDOR_TRANSACTIONS."USER_ID" AS VENDOR_TRANSACTIONS_USER_ID,
     AM_GB_LOCATION."LOCATION" AS AM_GB_LOCATION_LOCATION,
     am_ad_vendor."Vendor_Name" AS am_ad_vendor_Vendor_Name,
     ST_GL_PROJECT."DESCRIPTION" AS ST_GL_PROJECT_DESCRIPTION
FROM
     "dbo"."AM_GB_LOCATION" AM_GB_LOCATION INNER JOIN "dbo"."VENDOR_TRANSACTIONS" VENDOR_TRANSACTIONS ON AM_GB_LOCATION.LOCATION_ID = VENDOR_TRANSACTIONS."LOCATION"
