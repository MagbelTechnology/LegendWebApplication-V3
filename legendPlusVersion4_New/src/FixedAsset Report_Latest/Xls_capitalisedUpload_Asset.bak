	 AM_ASSET."BAR_CODE" AS AM_ASSET_BAR_CODE,
     AM_ASSET."Asset_User" AS AM_ASSET_Asset_User
FROM
     "dbo"."am_ad_branch" am_ad_branch INNER JOIN "dbo"."AM_ASSET" AM_ASSET ON am_ad_branch."BRANCH_CODE" = AM_ASSET."BRANCH_CODE"
     INNER JOIN "dbo"."am_ad_category" am_ad_category ON AM_ASSET."CATEGORY_CODE" = am_ad_category."category_code"
     INNER JOIN "dbo"."am_ad_department" am_ad_department ON AM_ASSET."BRANCH_CODE" = am_ad_department."Dept_code",
     "dbo"."am_gb_company" am_gb_company
WHERE
     am_ad_branch."BRANCH_CODE" = AM_ASSET."BRANCH_CODE"
     and AM_ASSET."Asset_Status" = 'ACTIVE' $P!{filterClause}
ORDER BY
     AM_ASSET."BRANCH_CODE" ASC,
     AM_ASSET."CATEGORY_CODE" ASC
]]></queryString>

