	<queryString>
		<![CDATA[SELECT SUBSTRING(CARDNUMBER, 1, 6) BIN, COUNT(1) as Counter, SUM(TRANSACTIONAMOUNT) as AmountValue , MONT_TRANS
FROM (
SELECT RAWDATARECIPIENT,CARDNUMBER, SUBSTRING(ONLINESETTLEMENTDATE, 4,8) MONT_TRANS, SUBSTRING(CARDNUMBER,1,6) BIN, TRANSACTIONAMOUNT FROM VATMSETTLEDTRANXNEW
WHERE
SUBSTRING(CARDNUMBER, 1, 6) IN (select BIN from trawdatabin where bankfiid = '01' AND upper(businesstype) = upper('ISSUING') ) AND
SUBSTRING(RAWDATARECIPIENT,1,6)  IN (select acquiringbin from tbankinfo where bankfiid = '01') AND TRANSACTIONAMOUNT <> 0
AND (ONLINESETTLEMENTDATE BETWEEN '01-01-2011' AND '31-03-2011') AND SETTLEMENTMENTSERVICESELECTED = '8') as t
group by SUBSTRING(CARDNUMBER, 1, 6),MONT_TRANS
]]>
	</queryString>
