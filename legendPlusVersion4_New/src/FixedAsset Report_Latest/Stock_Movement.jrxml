	<queryString>
		<![CDATA[select
a.Item_code,
a.Trans_desc,
isnull((select a.Quantity*-1 where a.Quantity < 0),null) 'Debit',
isnull((select a.Quantity where a.Quantity > 0),null) 'Credit',
a.warehouse_code,
a.Trans_date,
a.userid,
a.Previous_Balance,
a.comp_code,
b.company_name,
c.name,
d.description
from ST_INVENTORY_HISTORY a, am_gb_company b, ST_WAREHOUSE c, ST_INVENTORY_ITEMS d
where a.warehouse_code = c.warehouse_code
and a.item_code = d.item_code
and a.Trans_desc = 'TRANSFER'
 $P!{filterClause}
order by  a.warehouse_code, a.Trans_date, a.Trans_desc, a.userid, a.Item_code]]>
