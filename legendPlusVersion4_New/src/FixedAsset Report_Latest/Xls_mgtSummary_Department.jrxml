	<queryString>
		<![CDATA[SELECT
     am_gb_company."company_name" AS am_gb_company_company_name,
     am_Asset_local."Asset_id" AS am_Asset_local_Asset_id,
     am_Asset_local."Description" AS am_Asset_local_Description,
     am_ad_branch."BRANCH_NAME" AS am_ad_branch_BRANCH_NAME,
     am_ad_category."category_name" AS am_ad_category_category_name,
     am_Asset_local."Accum_dep" AS am_Asset_local_Accum_dep,
     am_Asset_local."monthly_dep" AS am_Asset_local_monthly_dep,
     am_Asset_local."Cost_Price" AS am_Asset_local_Cost_Price,
     am_Asset_local."NBV" AS am_Asset_local_NBV,
     am_Asset_local."Date_purchased" AS am_Asset_local_Date_purchased,
     am_ad_category."Dep_rate" AS am_ad_category_Dep_rate,
     am_ad_category."Accum_Dep_ledger" AS am_ad_category_Accum_Dep_ledger,
     am_ad_category."Dep_ledger" AS am_ad_category_Dep_ledger,
     am_ad_category."Asset_Ledger" AS am_ad_category_Asset_Ledger,
     am_ad_category."gl_account" AS am_ad_category_gl_account,
     am_ad_branch."BRANCH_CODE" AS am_ad_branch_BRANCH_CODE,
     am_ad_category."category_code" AS am_ad_category_category_code,
     am_ad_department."Dept_code" AS am_ad_department_Dept_code,
     am_ad_department."Dept_name" AS am_ad_department_Dept_name
FROM
     "dbo"."am_ad_branch" am_ad_branch INNER JOIN "dbo"."am_Asset_local" am_Asset_local ON am_ad_branch."BRANCH_CODE" = am_Asset_local."BRANCH_CODE"
     INNER JOIN "dbo"."am_ad_category" am_ad_category ON am_Asset_local."CATEGORY_CODE" = am_ad_category."category_code"
     INNER JOIN "dbo"."am_ad_department" am_ad_department ON am_Asset_local."DEPT_CODE" = am_ad_department."Dept_code",
     "dbo"."am_gb_company" am_gb_company
WHERE
     am_Asset_local."Asset_Status" = 'ACTIVE'
     and am_Asset_local."Cost_Price" > (am_gb_company."Cost_Threshold"-0.01) $P!{filterClause}
ORDER BY
     am_Asset_local.BRANCH_CODE ASC,
	 am_ad_department.Dept_name ASC,
     am_Asset_local.CATEGORY_CODE ASC]]>
