	<queryString>
		<![CDATA[SELECT
     am_gb_company."company_name" AS am_gb_company_company_name,
     am_gb_User."User_Name" AS am_gb_User_User_Name,
     am_gb_User."Full_Name" AS am_gb_User_Full_Name,
     art."Description" AS am_Raisentry_Transaction_Description,
     art."debitAccount" AS am_Raisentry_Transaction_debitAccount,
     art."creditAccount" AS am_Raisentry_Transaction_creditAccount,
     art."amount" AS am_Raisentry_Transaction_amount,
     art."iso" AS am_Raisentry_Transaction_iso,
     art."transaction_date" AS am_Raisentry_Transaction_transaction_date,
     art."asset_id" AS am_Raisentry_Transaction_asset_id,
     am_gb_User."User_id" AS am_gb_User_User_id,
     am_Error_Description."Description" AS am_Error_Description_Description,
     art."page1" AS am_Raisentry_Transaction_page1,
     am_ad_branch."BRANCH_NAME" AS am_ad_branch_BRANCH_NAME,
     am_ad_branch."BRANCH_CODE" AS am_ad_branch_BRANCH_CODE
FROM
     "dbo"."am_gb_User" am_gb_User INNER JOIN "dbo"."am_Raisentry_Transaction" art ON am_gb_User."User_id" = art."User_id"
     INNER JOIN "dbo"."am_Error_Description" am_Error_Description ON art."iso" = am_Error_Description."Error_code"
     INNER JOIN "dbo"."am_ad_branch" am_ad_branch ON am_gb_User."Branch" = am_ad_branch."BRANCH_ID",
     "dbo"."am_gb_company" am_gb_company
WHERE
     art."iso" <> '' $P!{filterClause}]]>
