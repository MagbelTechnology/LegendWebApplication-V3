	<queryString>
		<![CDATA[SELECT a.asset_id,c.User_id, c.full_name AS initiatorName,
CASE WHEN SUBSTRING(creditAccount,9,8) = '16330003' THEN 'Stock Account'
             ELSE 'Asset Aquisition Account' END AS creditAccountName,
CASE WHEN SUBSTRING(debitAccount,9,8) = '16330003' THEN 'Stock Account'
             ELSE 'Asset Aquisition Account' END AS debitAccountName,
d.full_name AS supervisorName,a.creditAccount,debitAccount,a.Description,a.amount,c.Full_Name,a.transaction_date FROM posted_transaction_view a, am_gb_User c, am_gb_User d
WHERE a.initiatorId = c.User_id
AND a.supervisorId = d.User_id
AND a.Description like '%Stock Issuance%'
$P!{filterClause}]]>
