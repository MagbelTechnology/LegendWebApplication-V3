	<parameter name="filterClause" class="java.lang.String"/>
	<queryString>
		<![CDATA[select ROUND(CAST(DATEDIFF(MONTH, effective_date, $P!{filterClause}) AS float) -
  (DATEPART(dd,effective_date)*1.0 - 1.0) / DAY(EOMONTH(effective_date)) +
  ((DATEPART(dd,$P!{filterClause})*1.0 ) / DAY(EOMONTH($P!{filterClause}))),0) AS USED,Monthly_Dep, USEFUL_LIFE,effective_date,
  (ROUND(CAST(DATEDIFF(MONTH, effective_date, $P!{filterClause}) AS float) -
  (DATEPART(dd,effective_date)*1.0 - 1.0) / DAY(EOMONTH(effective_date)) +
  ((DATEPART(dd,$P!{filterClause})*1.0 ) / DAY(EOMONTH($P!{filterClause}))),0)) - USEFUL_LIFE AS DIFF,ASSET_ID, DESCRIPTION, NBV,
  a.BRANCH_CODE,a.CATEGORY_CODE,b.BRANCH_NAME,c.category_name from AM_ASSET a, am_ad_branch b, am_ad_category c
  WHERE ASSET_STATUS = 'ACTIVE' AND NBV > 20
  AND (ROUND(CAST(DATEDIFF(MONTH, effective_date, $P!{filterClause}) AS float) -
  (DATEPART(dd,effective_date)*1.0 - 1.0) / DAY(EOMONTH(effective_date)) +
  ((DATEPART(dd,$P!{filterClause})*1.0 ) / DAY(EOMONTH($P!{filterClause}))),0)) - USEFUL_LIFE  <> 0
  AND a.BRANCH_CODE = b.BRANCH_CODE
  AND a.CATEGORY_CODE = c.CATEGORY_CODE]]>
	</queryString>
