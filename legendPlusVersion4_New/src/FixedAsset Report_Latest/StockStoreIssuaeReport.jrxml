<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="StockStoreIssuaeReport" language="groovy" pageWidth="595" pageHeight="842" columnWidth="575" leftMargin="0" rightMargin="20" topMargin="20" bottomMargin="20">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="filterClause" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT distinct a.STOCK_CODE, a.description,a.Cost_Price,a.QUANTITY_DELIVER,c.CATEGORY_CODE,d.category_name FROM ST_DISTRIBUTION_ITEM a,ST_DISTRIBUTION_ORDER b, ST_STOCK c, ST_STOCK_CATEGORY d
WHERE a.PROJECT_CODE = b.BATCH_ID
AND a.STOCK_CODE = c.Asset_id
AND c.CATEGORY_CODE = d.Category_Code
$P!{filterClause}]]>
	</queryString>
	<field name="STOCK_CODE" class="java.lang.String"/>
	<field name="description" class="java.lang.String"/>
	<field name="Cost_Price" class="java.math.BigDecimal"/>
	<field name="QUANTITY_DELIVER" class="java.lang.Integer"/>
	<field name="CATEGORY_CODE" class="java.lang.String"/>
	<field name="category_name" class="java.lang.String"/>
	<variable name="totalcategory" class="java.math.BigDecimal" resetType="Group" resetGroup="CATEGORY" calculation="Sum">
		<variableExpression><![CDATA[$F{Cost_Price}]]></variableExpression>
	</variable>
	<group name="CATEGORY" isStartNewPage="true" isResetPageNumber="true" isReprintHeaderOnEachPage="true">
		<groupExpression><![CDATA[$F{CATEGORY_CODE}]]></groupExpression>
		<groupHeader>
			<band height="21">
				<staticText>
					<reportElement x="0" y="1" width="330" height="20"/>
					<textElement/>
					<text><![CDATA[DESCRIPTION]]></text>
				</staticText>
				<staticText>
					<reportElement x="330" y="1" width="81" height="20"/>
					<textElement/>
					<text><![CDATA[QUANTITY]]></text>
				</staticText>
				<staticText>
					<reportElement x="411" y="1" width="124" height="20"/>
					<textElement/>
					<text><![CDATA[COST PRICE]]></text>
				</staticText>
			</band>
		</groupHeader>
		<groupFooter>
			<band/>
		</groupFooter>
	</group>
	<background>
		<band splitType="Stretch"/>
	</background>
	<title>
		<band splitType="Stretch"/>
	</title>
	<pageHeader>
		<band splitType="Stretch"/>
	</pageHeader>
	<columnHeader>
		<band height="21" splitType="Stretch">
			<textField>
				<reportElement x="0" y="1" width="330" height="20"/>
				<textElement/>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{category_name}]]></textFieldExpression>
			</textField>
		</band>
	</columnHeader>
	<detail>
		<band height="21" splitType="Stretch">
			<textField>
				<reportElement x="0" y="1" width="330" height="20"/>
				<textElement/>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{description}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="330" y="1" width="81" height="20"/>
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.lang.Integer"><![CDATA[$F{QUANTITY_DELIVER}]]></textFieldExpression>
			</textField>
			<textField pattern="#,##0.00">
				<reportElement x="411" y="1" width="124" height="20"/>
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.math.BigDecimal"><![CDATA[$F{Cost_Price}]]></textFieldExpression>
			</textField>
		</band>
	</detail>
	<columnFooter>
		<band height="20" splitType="Stretch">
			<textField pattern="#,##0.00">
				<reportElement isPrintRepeatedValues="false" x="411" y="0" width="124" height="20" printWhenGroupChanges="CATEGORY"/>
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.math.BigDecimal"><![CDATA[$V{totalcategory}]]></textFieldExpression>
			</textField>
			<staticText>
				<reportElement x="0" y="0" width="330" height="20"/>
				<textElement/>
				<text><![CDATA[Total]]></text>
			</staticText>
		</band>
	</columnFooter>
	<pageFooter>
		<band splitType="Stretch"/>
	</pageFooter>
	<summary>
		<band splitType="Stretch"/>
	</summary>
</jasperReport>
