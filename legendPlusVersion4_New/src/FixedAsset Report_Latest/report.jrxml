d.Dept_Name AS [Department Name]
FROM AM_ASSET AS a JOIN am_ad_branch AS b
ON a.Branch_ID = b.BRANCH_ID
JOIN am_ad_category AS c ON a.Category_ID = c.category_ID
JOIN am_ad_department AS d ON a.Dept_ID = d.Dept_ID;]]>
