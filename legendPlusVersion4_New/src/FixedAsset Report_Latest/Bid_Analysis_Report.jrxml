	<queryString>
		<![CDATA[SELECT
     am_gb_company."company_name" AS am_gb_company_company_name,
     am_gb_company."company_code" AS am_gb_company_company_code,
     am_gb_company."company_address" AS am_gb_company_company_address,
     PR_RFQ_RESPONSE."ReqnID" AS PR_RFQ_RESPONSE_ReqnID,
     PR_RFQ_RESPONSE."UserID" AS PR_RFQ_RESPONSE_UserID,
     PR_RFQ_RESPONSE."Status" AS PR_RFQ_RESPONSE_Status,
     PR_RFQ_RESPONSE."Company_Code" AS PR_RFQ_RESPONSE_Company_Code,
     PR_RFQ_RESPONSE."Vendor_Code" AS PR_RFQ_RESPONSE_Vendor_Code,
     PR_RFQ_RESPONSE."Quoted_Vendor_code" AS PR_RFQ_RESPONSE_Quoted_Vendor_code,
     PR_RFQ_RESPONSE."QuotedBy" AS PR_RFQ_RESPONSE_QuotedBy,
     PR_RFQ_RESPONSE."Job_Title" AS PR_RFQ_RESPONSE_Job_Title,
     PR_RFQ_RESPONSE."Email" AS PR_RFQ_RESPONSE_Email,
     PR_RFQ_RESPONSE."DaysQuote_Valid" AS PR_RFQ_RESPONSE_DaysQuote_Valid,
     PR_RFQ_RESPONSE."Remark" AS PR_RFQ_RESPONSE_Remark,
     PR_RFQ_RESPONSE_DETAIL."Description" AS PR_RFQ_RESPONSE_DETAIL_Description,
     PR_RFQ_RESPONSE_DETAIL."Delivery_Date" AS PR_RFQ_RESPONSE_DETAIL_Delivery_Date,
     PR_RFQ_RESPONSE_DETAIL."Quantity" AS PR_RFQ_RESPONSE_DETAIL_Quantity,
     PR_RFQ_RESPONSE_DETAIL."Unit_Price" AS PR_RFQ_RESPONSE_DETAIL_Unit_Price,
     PR_RFQ_RESPONSE_DETAIL."Cost_Price" AS PR_RFQ_RESPONSE_DETAIL_Cost_Price,
     am_ad_vendor."Vendor_Name" AS am_ad_vendor_Vendor_Name,
     am_ad_vendor."Vendor_Status" AS am_ad_vendor_Vendor_Status,
     PR_RFQ_RESPONSE_DETAIL."Item_No" AS PR_RFQ_RESPONSE_DETAIL_Item_No,
     PR_RFQ_RESPONSE_DETAIL."ReqnID" AS PR_RFQ_RESPONSE_DETAIL_ReqnID
FROM
     "dbo"."PR_RFQ_RESPONSE_DETAIL" PR_RFQ_RESPONSE_DETAIL RIGHT OUTER JOIN "dbo"."PR_RFQ_RESPONSE" PR_RFQ_RESPONSE ON PR_RFQ_RESPONSE_DETAIL."Vendor_Code" = PR_RFQ_RESPONSE."Vendor_Code"
     AND PR_RFQ_RESPONSE_DETAIL."ReqnID" = PR_RFQ_RESPONSE."ReqnID"
     INNER JOIN "dbo"."am_ad_vendor" am_ad_vendor ON PR_RFQ_RESPONSE."Vendor_Code" = am_ad_vendor."Vendor_Code",
     "dbo"."am_gb_company" am_gb_company
WHERE
     PR_RFQ_RESPONSE_DETAIL."Unit_Price" <> 0
     and PR_RFQ_RESPONSE."ReqnID" = $P{ReqnId}]]>
