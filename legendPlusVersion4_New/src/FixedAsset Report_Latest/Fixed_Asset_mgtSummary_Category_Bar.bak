     am_ad_category."Asset_Ledger" AS am_ad_category_Asset_Ledger,
     am_ad_category."gl_account" AS am_ad_category_gl_account,
     am_ad_branch."BRANCH_CODE" AS am_ad_branch_BRANCH_CODE
FROM
     "dbo"."am_ad_branch" am_ad_branch INNER JOIN "dbo"."am_Asset" am_Asset ON am_ad_branch."BRANCH_CODE" = am_Asset."BRANCH_CODE"
     INNER JOIN "dbo"."am_ad_category" am_ad_category ON am_Asset."CATEGORY_CODE" = am_ad_category."category_code",
     "dbo"."am_gb_company" am_gb_company 
where am_Asset."Asset_Status" = 'ACTIVE' and upper(am_Asset."Req_Depreciation") = 'Y' and am_Asset."Cost_Price" >  (am_gb_company."Cost_Threshold" - 0.01) $P!{filterClause} order by am_ad_category_category_name,am_ad_branch_BRANCH_CODE]]></queryString>

