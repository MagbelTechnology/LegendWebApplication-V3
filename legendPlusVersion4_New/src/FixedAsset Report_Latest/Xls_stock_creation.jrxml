	<queryString>
		<![CDATA[SELECT
     am_gb_company."company_name" AS am_gb_company_company_name,
     st_stock."Asset_id" AS st_stock_Asset_id,
     st_stock."Description" AS st_stock_Description,
     am_ad_branch."BRANCH_NAME" AS am_ad_branch_BRANCH_NAME,
     am_ad_category."category_name" AS am_ad_category_category_name,
     st_stock."Accum_dep" AS st_stock_Accum_dep,
     st_stock."monthly_dep" AS st_stock_monthly_dep,
     st_stock."Cost_Price" AS st_stock_Cost_Price,
     st_stock."NBV" AS st_stock_NBV,
     st_stock."IMPROV_COST" AS st_stock_IMPROV_COST,
     st_stock."IMPROV_ACCUMDEP" AS st_stock_IMPROV_ACCUMDEP,
     st_stock."IMPROV_MONTHLYDEP" AS st_stock_IMPROV_MONTHLYDEP,
     st_stock."IMPROV_NBV" AS st_stock_IMPROV_NBV,
     st_stock."TOTAL_NBV" AS st_stock_TOTAL_NBV,
     st_stock."Date_purchased" AS st_stock_Date_purchased,
     am_ad_category."Dep_rate" AS am_ad_category_Dep_rate,
     am_ad_category."Accum_Dep_ledger" AS am_ad_category_Accum_Dep_ledger,
     am_ad_category."Dep_ledger" AS am_ad_category_Dep_ledger,
     am_ad_category."Asset_Ledger" AS am_ad_category_Asset_Ledger,
     am_ad_category."gl_account" AS am_ad_category_gl_account,
     st_stock."dep_end_date" AS st_stock_dep_end_date,
     st_stock."effective_date" AS st_stock_effective_date,
     st_stock."Posting_Date" AS st_stock_Posting_Date,
     am_ad_department."Dept_name" AS am_ad_department_Dept_name,
     sbu_setup."sbu_name" AS sbu_setup_sbu_name,
     st_stock."asset_user" AS st_stock_asset_user,
     st_stock."Asset_Status" AS st_stock_Asset_Status
FROM
     "dbo"."am_ad_branch" am_ad_branch INNER JOIN "dbo"."st_stock" st_stock ON am_ad_branch."BRANCH_CODE" = st_stock."BRANCH_CODE"
     INNER JOIN "dbo"."am_ad_category" am_ad_category ON st_stock."CATEGORY_CODE" = am_ad_category."category_code"
     LEFT OUTER JOIN "dbo"."am_ad_department" am_ad_department ON st_stock."BRANCH_CODE" = am_ad_department."Dept_code"
     LEFT OUTER JOIN "dbo"."sbu_setup" sbu_setup ON st_stock."SBU_CODE" = sbu_setup."sbu_code"
     LEFT OUTER JOIN "dbo"."am_ad_section" am_ad_section ON st_stock."SECTION_CODE" = am_ad_section."Section_Code"
     LEFT OUTER JOIN "dbo"."am_gb_assetMake" am_gb_assetMake ON st_stock."asset_make" = am_gb_assetMake."AssetMake_ID"
     LEFT OUTER JOIN "dbo"."am_ad_vendor" am_ad_vendor ON st_stock."supplier_name" = am_ad_vendor."Vendor_ID"
     LEFT OUTER JOIN "dbo"."am_ad_vendor" am_ad_vendor_A ON st_stock."asset_maintenance" = am_ad_vendor_A."Vendor_ID",
     "dbo"."am_gb_company" am_gb_company

WHERE
     (st_stock."Asset_Status" = 'ACTIVE'
     or st_stock."Asset_Status" = 'APPROVED')
     and st_stock."Cost_Price" > (am_gb_company."Cost_Threshold"-0.01)
 $P!{filterClause}
ORDER BY
     st_stock."BRANCH_CODE" ASC,
     st_stock."CATEGORY_CODE" ASC]]>
