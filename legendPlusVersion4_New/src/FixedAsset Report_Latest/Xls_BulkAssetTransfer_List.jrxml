     am_gb_bulkTransfer."Description" AS am_gb_bulkTransfer_Description,
     am_gb_bulkTransfer."oldbranch_id" AS am_gb_bulkTransfer_oldbranch_id,
     am_gb_bulkTransfer."olddept_id" AS am_gb_bulkTransfer_olddept_id,
     am_gb_bulkTransfer."oldSBU_CODE" AS am_gb_bulkTransfer_oldSBU_CODE,
     am_gb_bulkTransfer."oldsection_id" AS am_gb_bulkTransfer_oldsection_id,
     am_gb_bulkTransfer."oldAsset_User" AS am_gb_bulkTransfer_oldAsset_User,
     am_gb_bulkTransfer."newbranch_id" AS am_gb_bulkTransfer_newbranch_id,
     am_gb_bulkTransfer."newdept_id" AS am_gb_bulkTransfer_newdept_id,
     am_gb_bulkTransfer."newSBU_CODE" AS am_gb_bulkTransfer_newSBU_CODE,
     am_gb_bulkTransfer."newsection_id" AS am_gb_bulkTransfer_newsection_id,
     am_gb_bulkTransfer."newAsset_User" AS am_gb_bulkTransfer_newAsset_User,
     am_gb_bulkTransfer."Batch_id" AS am_gb_bulkTransfer_Batch_id,
     am_gb_bulkTransfer."Transferby_id" AS am_gb_bulkTransfer_Transferby_id,
     am_gb_bulkTransfer."CATEGORY_ID" AS am_gb_bulkTransfer_CATEGORY_ID,
     am_gb_bulkTransfer."NEW_ASSET_ID" AS am_gb_bulkTransfer_NEW_ASSET_ID,
     am_gb_bulkTransfer."ACCUM_DEP" AS am_gb_bulkTransfer_ACCUM_DEP,
     am_gb_bulkTransfer."MONTHLY_DEP" AS am_gb_bulkTransfer_MONTHLY_DEP,
     am_gb_bulkTransfer."NBV" AS am_gb_bulkTransfer_NBV,
     am_gb_bulkTransfer."TRANSFER_DATE" AS am_gb_bulkTransfer_TRANSFER_DATE,
     am_gb_bulkTransfer."transId" AS am_gb_bulkTransfer_transId,
     am_gb_bulkTransfer."TRANSFER_COST" AS am_gb_bulkTransfer_TRANSFER_COST,
     am_gb_bulkTransfer."asset_code" AS am_gb_bulkTransfer_asset_code,
     am_ad_branch."BRANCH_NAME" AS am_ad_branch_BRANCH_NAME,
     am_ad_branch_A."BRANCH_NAME" AS am_ad_branch_A_BRANCH_NAME,
     am_gb_User."Full_Name" AS am_gb_User_Full_Name,
     am_gb_User."User_Name" AS am_gb_User_User_Name,
     am_gb_bulkTransfer."COST_PRICE" AS am_gb_bulkTransfer_COST_PRICE
FROM
     "dbo"."am_ad_branch" am_ad_branch INNER JOIN "dbo"."am_gb_bulkTransfer" am_gb_bulkTransfer ON am_ad_branch."BRANCH_ID" = am_gb_bulkTransfer."oldbranch_id"
     INNER JOIN "dbo"."am_ad_branch" am_ad_branch_A ON am_gb_bulkTransfer."newbranch_id" = am_ad_branch_A."BRANCH_ID"
     INNER JOIN "dbo"."am_gb_User" am_gb_User ON am_gb_bulkTransfer."Transferby_id" = am_gb_User."User_id"
$P!{filterClause}
ORDER BY
     Batch_id ASC]]>
